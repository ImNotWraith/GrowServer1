{
  "name": "growserver",
  "version": "2.3.0",
  "description": "a growtopia private server",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/app.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "lint": "biome check ./src",
    "dev": "rimraf dist && rimraf assets/cache && npm run build && npm start",
    "install": "(node scripts/setup.js) && npm run migrate && npm run seed && npm run finish",
    "finish": "echo \u001b[46mSetup Completed!\u001b[0m && echo \u001b[46mCheck out GrowServer github: https://github.com/JadlionHD/GrowServer\u001b[0m"
  },
  "author": "JadlionHD <jadlion.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.5.2",
    "@types/crypto-js": "^4.1.1",
    "@types/decompress": "^4.2.7",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "body-parser": "^1.20.1",
    "chalk": "4.1.1",
    "crypto-js": "^4.1.1",
    "decompress": "^4.2.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "growtopia.js": "^1.3.4",
    "knex": "^2.5.1"
  }
}
