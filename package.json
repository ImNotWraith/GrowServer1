{
  "name": "growserver",
  "version": "2.4.0",
  "description": "a growtopia private server",
  "main": "dist/app.js",
  "type": "module",
  "scripts": {
    "build": "npm run lint && swc ./src -d dist",
    "start": "vite build --logLevel silent && node -r dotenv/config dist/app.js",
    "seed": "node -r dotenv/config scripts/seeds.js",
    "generate": "drizzle-kit push --config=drizzle.config.ts",
    "studio": "drizzle-kit studio",
    "lint": "eslint lint ./src",
    "iteminfo:fetch": "node wiki-scraper/scrape.js",
    "dev": "rimraf dist && rimraf assets/cache && eslint ./src && tsc && vite build --logLevel silent && node -r dotenv/config dist/app.js",
    "dev:server": "rimraf dist && rimraf assets/cache && eslint ./src && tsc && node -r dotenv/config dist/app.js",
    "dev:nolint": "rimraf dist && rimraf assets/cache && tsc && vite build --logLevel silent && node -r dotenv/config dist/app.js",
    "dev:vite": "vite",
    "install": "(node scripts/setup.js) && tsc && vite build --logLevel silent && npm run generate && npm run seed && npm run finish",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "finish": "echo \u001b[46mSetup Completed!\u001b[0m && echo \u001b[46mCheck out GrowServer github: https://github.com/JadlionHD/GrowServer\u001b[0m"
  },
  "author": "JadlionHD <jadlion.dev@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/better-sqlite3": "^7.6.10",
    "@types/chance": "^1.1.6",
    "@types/crypto-js": "^4.2.2",
    "@types/decompress": "^4.2.7",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.19",
    "bits-ui": "^0.21.11",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.22.8",
    "formsnap": "^1.0.1",
    "globals": "^15.6.0",
    "lucide-svelte": "^0.399.0",
    "mode-watcher": "^0.3.1",
    "postcss": "^8.4.39",
    "rimraf": "^5.0.7",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "svelte-sonner": "^0.3.24",
    "sveltekit-superforms": "^2.15.2",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^4.9.5",
    "typescript-eslint": "^7.14.1",
    "vaul-svelte": "^0.3.2",
    "vite": "^5.3.2",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "better-sqlite3": "^11.1.1",
    "chalk": "5.3.0",
    "chance": "^1.1.11",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "decompress": "^4.2.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "embla-carousel-svelte": "^8.1.5",
    "eris": "^0.17.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "growtopia.js": "^1.5.2",
    "nanoid": "^5.0.7",
    "ws": "^8.17.1"
  }
}
